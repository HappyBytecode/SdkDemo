<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AllDerectionIcon">
        <!-- icon大小 -->
        <attr name="left_size" format="dimension|reference" />
        <attr name="right_size" format="dimension|reference" />
        <attr name="top_size" format="dimension|reference" />
        <attr name="bottom_size" format="dimension|reference" />
        <!-- icon资源 -->
        <attr name="left_icon" format="string|reference" />
        <attr name="right_icon" format="string|reference" />
        <attr name="top_icon" format="string|reference" />
        <attr name="bottom_icon" format="string|reference" />
        <!-- icon颜色 -->
        <attr name="left_color" format="color|reference" />
        <attr name="right_icon_color" format="color|reference" />
        <attr name="top_color" format="color|reference" />
        <attr name="bottom_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="IconPlus">
        <attr name="icon_normal" format="string|reference" />
        <attr name="color_normal" format="color|reference" />
        <attr name="size_normal" format="dimension|reference" />
        <!-- 按压后显示的icon颜色 -->
        <attr name="color_pressed" format="color|reference" />
        <attr name="color_selected" format="color|reference" />
        <!-- isSelect=true时显示的icon -->
        <attr name="icon_selected" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="Start">
        <attr name="start_icon" format="color|reference" />
        <attr name="start_half_icon" format="color|reference" />
        <attr name="start_empty_icon" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="HXLayout">
        <attr name="android_maxWidth" format="dimension" />
        <attr name="android_maxHeight" format="dimension" />
        <attr name="android_minWidth" format="dimension" />
        <attr name="android_minHeight" format="dimension" />
        <attr name="hx_radius" format="dimension" />
        <attr name="hx_shadowElevation" format="dimension" />
        <attr name="hx_shadowAlpha" format="float" />
        <attr name="hx_shadowColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TipDialogView">
        <attr name="tip_dialog_min_width" format="dimension" />
        <attr name="tip_dialog_min_height" format="dimension" />
        <attr name="tip_dialog_max_width" format="dimension" />
        <attr name="tip_dialog_padding_vertical" format="dimension" />
        <attr name="tip_dialog_padding_horizontal" format="dimension" />
        <attr name="tip_dialog_radius" format="dimension" />
        <attr name="tip_dialog_loading_size" format="dimension" />
        <attr name="tip_dialog_text_margin_top" format="dimension" />
        <attr name="tip_dialog_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="VerticalTabLayout">
        <attr name="indicator_color" format="color" />
        <attr name="indicator_width" format="dimension" />
        <attr name="indicator_height" format="dimension" />
        <attr name="indicator_corners" format="dimension" />
        <attr name="indicator_gravity" format="enum">
            <enum name="left" value="3" />
            <enum name="right" value="5" />
            <enum name="fill" value="119" />
        </attr>
        <attr name="tab_margin" format="dimension" />
        <attr name="tab_mode" format="enum">
            <enum name="fixed" value="10" />
            <enum name="scrollable" value="11" />
        </attr>
        <attr name="tab_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="EnhanceTabLayout">
        <attr name="tab_Mode" format="enum">
            <enum name="mode_fixed" value="1" />
            <enum name="mode_scrollable" value="2" />
        </attr>
        <attr name="tabIndicatorColor" format="color" />
        <attr name="tabSelectTextColor" format="color" />
        <attr name="tabTextColor" format="color" />
        <attr name="tabIndicatorHeight" format="dimension" />
        <attr name="tabIndicatorWidth" format="dimension" />
        <attr name="tabTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PlusMinusView">
        <attr name="plusType" format="string" />
    </declare-styleable>

    <declare-styleable name="RadarView">
        <attr name="sweepColor" format="color" />
        <attr name="sweepSpeed" format="float" />
        <attr name="circleStartColor" format="color" />
        <attr name="circleEndColor" format="color" />
    </declare-styleable>

    <declare-styleable name="OvalPathView">
        <attr name="ovalPathBackgroundColor" format="color" />
        <attr name="ovalPathProgressColor" format="color" />
        <attr name="ovalPathViewText" format="string" />
        <attr name="ovalPathViewTextColor" format="color" />
        <attr name="ovalPathViewTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArcProgressBar">
        <attr name="arcBgColor" format="color" />
        <attr name="arcForeStartColor" format="color" />
        <attr name="arcForeEndColor" format="color" />
        <attr name="dottedDefaultColor" format="color" />
        <attr name="dottedRunColor" format="color" />
        <attr name="dottedLineCount" format="integer" />
        <attr name="dottedLineWidth" format="integer" />
        <attr name="dottedLineHeight" format="integer" />
        <attr name="lineDistance" format="integer" />
        <attr name="progressMax" format="integer" />
        <attr name="arcUseGradient" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RingStatisticsView">
        <attr name="ringWidth" format="reference|dimension" />
        <attr name="ringNumTextSize" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="chartView">
        <!-- xy坐标轴颜色 -->
        <attr name="xylinecolor" format="color" />
        <!-- xy坐标轴宽度 -->
        <attr name="xylinewidth" format="dimension" />
        <!-- 折线图中折线的宽度 -->
        <attr name="linewidth" format="dimension" />
        <!-- 选中时候竖线的宽度 -->
        <attr name="verticallinewidth" format="dimension" />
        <!-- xy坐标轴文字颜色 -->
        <attr name="xytextcolor" format="color" />
        <!-- xy坐标轴文字大小 -->
        <attr name="xytextsize" format="dimension" />
        <!-- 折线图中折线的颜色 -->
        <attr name="linecolor" format="color" />
        <!-- 选中时竖线的颜色 -->
        <attr name="verticallinecolor" format="color" />
        <!-- x轴各个坐标点水平间距 -->
        <attr name="interval" format="dimension" />
        <!-- 背景颜色 -->
        <attr name="bgcolor" format="color" />
        <!--是否在ACTION_UP时，根据速度进行自滑动，建议关闭，过于占用GPU-->
        <attr name="isScroll" format="boolean" />
    </declare-styleable>

    <!-- 自定义HeadView，说明：如果"右键图标"和"右键文字"都有设置，仅显示文字，不显示图标 -->
    <declare-styleable name="HeadView">
        <!-- 标题内容 -->
        <attr name="title_text" format="string|reference" />
        <!-- 标题大小 -->
        <attr name="title_size" format="dimension|reference" />
        <!-- 标题颜色 -->
        <attr name="title_color" format="color|reference" />

        <!-- 左键图标 -->
        <attr name="left_image" format="reference" />
        <!-- 左键是否显示，true-显示，false-隐藏 -->
        <attr name="left_visible" format="boolean|reference" />
        <attr name="left_image_tint" format="color" />
        <!-- 右键图标 -->
        <attr name="right_image" format="reference" />

        <!-- 右键显示的内容 -->
        <attr name="right_text" format="string|reference" />
        <!-- 右键内容的颜色 -->
        <attr name="right_color" format="color|reference" />
        <attr name="right_visible" format="boolean" />
        <attr name="divide_line_visible" format="boolean|reference" />
    </declare-styleable>

    <!-- CircleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <!-- TextView Plus -->
    <declare-styleable name="TextViewPlus">
        <attr name="left_height" format="dimension" />
        <attr name="left_width" format="dimension" />
        <attr name="right_height" format="dimension" />
        <attr name="right_width" format="dimension" />
        <attr name="top_height" format="dimension" />
        <attr name="top_width" format="dimension" />
        <attr name="bottom_height" format="dimension" />
        <attr name="bottom_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TimeTable">
        <attr name="time_type" format="integer" />
    </declare-styleable>

    <!--进度条样式-->
    <declare-styleable name="NumberProgressBar">
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />
    </declare-styleable>

    <!--sweet_alert-->
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction" />
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
</resources>
